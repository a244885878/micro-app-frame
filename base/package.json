{
  "name": "fa-chan-base",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --mode development",
    "test": "vite --mode test",
    "pro": "vite --mode production",
    "build:dev": "vue-tsc -b && vite build --mode development",
    "build:test": "vue-tsc -b && vite build --mode test",
    "build:pro": "vue-tsc -b && vite build --mode production",
    "gen:api": "node ./openapi.js",
    "preview": "vite preview",
    "lint": "eslint --fix --ext .js,.ts,.vue ./src",
    "prepare": "husky"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@micro-zoe/micro-app": "1.0.0-rc.21",
    "axios": "^1.8.0",
    "element-plus": "^2.9.5",
    "nprogress": "^0.2.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.23.3",
    "@types/nprogress": "^0.2.3",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@umijs/openapi": "^1.13.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^39.1.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.18.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "prettier": "^3.1.0",
    "prettier-eslint": "^16.1.2",
    "sass": "^1.85.1",
    "tslib": "^2.8.1",
    "typescript": "~5.3.3",
    "vite": "^6.2.0",
    "vite-plugin-eslint": "^1.8.1",
    "vue-eslint-parser": "^9.3.2",
    "vue-tsc": "^2.2.4"
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx,jsx,vue}": [
      "npm run lint",
      "prettier --write"
    ],
    "src/**/*.{css,less,scss}": [
      "prettier --write"
    ],
    "src/**/*.{md,json}": [
      "prettier --write"
    ]
  }
}
